info\n      })\n      .catch((err) => console.log(err));\n  };\n\n  useEffect(() => {\n    getBounties();\n  }, []);\n  return (\n    <div>\n      <Header />\n      <BountyForm submit={postBounty} btnText=\"Submit Bounty!\" />\n      {bounty.map((bounty) => (\n        <Bounty\n          {...bounty} // passing id\n          key={bounty.firstName}\n          deleteBounty={deleteBounty}\n          updateBounty={updateBounty}\n        />\n      ))}\n      <Footer />\n    </div>\n  );\n}\n","C:\\Users\\cvanl\\OneDrive - Bryan University\\fsw-125\\fsw-125\\week5\\frontend_to_server\\client\\src\\components\\bountyData\\BountyForm.js",[],"C:\\Users\\cvanl\\OneDrive - Bryan University\\fsw-125\\fsw-125\\week5\\frontend_to_server\\client\\src\\components\\bountyData\\Bounty.js",[],["32","33"],"C:\\Users\\cvanl\\OneDrive - Bryan University\\fsw-125\\fsw-125\\week5\\frontend_to_server\\client\\src\\components\\Semantics\\Header\\Header.js",[],"C:\\Users\\cvanl\\OneDrive - Bryan University\\fsw-125\\fsw-125\\week5\\frontend_to_server\\client\\src\\components\\Semantics\\Footer\\Footer.js",[],{"ruleId":"34","replacedBy":"35"},{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","severity":1,"message":"39","line":33,"column":52,"nodeType":"40","messageId":"41","endLine":33,"endColumn":54},{"ruleId":"34","replacedBy":"42"},{"ruleId":"36","replacedBy":"43"},"no-native-reassign",["44"],"no-negated-in-lhs",["45"],"eqeqeq","Expected '!==' and instead saw '!='.","BinaryExpression","unexpected",["44"],["45"],"no-global-assign","no-unsafe-negation"]